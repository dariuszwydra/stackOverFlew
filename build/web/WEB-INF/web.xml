<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>loginConfirm</servlet-name>
        <servlet-class>servlets.loginConfirm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registerConfirm</servlet-name>
        <servlet-class>servlets.registerConfirm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logoutConfirm</servlet-name>
        <servlet-class>servlets.logoutConfirm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addPost</servlet-name>
        <servlet-class>servlets.addPost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addAnswer</servlet-name>
        <servlet-class>servlets.addAnswer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddRating</servlet-name>
        <servlet-class>servlets.AddRating</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminLoginConfirm</servlet-name>
        <servlet-class>servlets.adminLoginConfirm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminLogoutConfirm</servlet-name>
        <servlet-class>servlets.adminLogoutConfirm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteAnswer</servlet-name>
        <servlet-class>servlets.deleteAnswer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deletePost</servlet-name>
        <servlet-class>servlets.deletePost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateAnswer</servlet-name>
        <servlet-class>servlets.updateAnswer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginConfirm</servlet-name>
        <url-pattern>/loginConfirm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registerConfirm</servlet-name>
        <url-pattern>/registerConfirm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logoutConfirm</servlet-name>
        <url-pattern>/logoutConfirm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addPost</servlet-name>
        <url-pattern>/addPost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addAnswer</servlet-name>
        <url-pattern>/addAnswer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddRating</servlet-name>
        <url-pattern>/AddRating</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminLoginConfirm</servlet-name>
        <url-pattern>/adminLoginConfirm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminLogoutConfirm</servlet-name>
        <url-pattern>/adminLogoutConfirm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteAnswer</servlet-name>
        <url-pattern>/deleteAnswer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deletePost</servlet-name>
        <url-pattern>/deletePost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateAnswer</servlet-name>
        <url-pattern>/updateAnswer</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
